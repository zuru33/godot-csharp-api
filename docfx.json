{
  "metadata": [
    {
        "src": [
          {
            "files": [
              "src/4.1/**.dll"
            ]
          }
        ],
        "dest": "4.1"
      }
  ],
  "build": {
    "content": [
      {
        "files": [
          "4.1/**.yml"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "README.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "docs",
    "globalMetadata": {
      "_appTitle": "Godot C# API",
      "_appLogoPath": "images/documentation_logo.svg",
      "_appFaviconPath": "images/documentation_logo.svg",
      "_appFooter": "<span>Generated by <strong>DocFX</strong></span>",
      "_enableSearch": true
    },
    "template": [
      "default",
      "templates/darkfx/darkfx",
      "templates/godot"
    ],
    "markdownEngineName": "markdig"
  }
}